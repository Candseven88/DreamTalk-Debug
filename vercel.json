{
    "version": 2,
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": ".*.html", "use": "@vercel/static" },
      { "src": "api/webhook.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/webhook", "dest": "api/webhook.js" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }